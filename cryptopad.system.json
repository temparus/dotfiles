{
    "mkdirs": [
        "/usr/local/bin",
        "/root/.local/bin",
        "/var/lib/iptables"
    ],
    "copy": {
        "portage/make-cryptopad.conf": "/etc/portage/make.conf",
        "scripts/vpn/vpn.openrc": "/etc/init.d/vpn",
        "scripts/vpn/network-changed.sh": "/root/.local/bin/network-changed.sh",
        "scripts/vpn/iptables-config.sh": "/usr/local/bin/iptables-config",
        "config/iptables/default.ipv4.rules": "/var/lib/iptables/default.ipv4.rules",
        "config/iptables/default.ipv6.rules": "/var/lib/iptables/default.ipv6.rules",
        "config/iptables/vpn-kill.ipv4.rules": "/var/lib/iptables/vpn-kill.ipv4.rules",
        "config/iptables/vpn-kill.ipv6.rules": "/var/lib/iptables/vpn-kill.ipv6.rules",
        "scripts/kernel-upgrade.sh": "/usr/local/bin/kernel-upgrade",
        "scripts/system-update.sh": "/usr/local/bin/system-update"
    }
}
